#include <iostream>
using namespace std;
int main()
{
    int num1 = 0,num2 = 1,num3 = 0;
 
   for (int i = 1; i <= 22; ++i)
   {
       if(i == 1)
       {
           cout << num1 << ",";
           continue;
       }
       if(i == 2)
       {
           cout <<num2 << ",";
           continue;
       }
      num3 = num1 +num2;
       num1 = num2;
       num2 =num3;
       
       cout <<num3 << ",";
   }
   return 0;
}
