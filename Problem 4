#include<iostream>
using namespace std;
int main()
{
   int n1;
   int n2;
   int n3;
   
   cout<< "First Value: "<<endl;
   cin>> n1;
   cout<< "Second Value: "<<endl;
   cin>> n2;
   cout<< "Third Value: "<<endl;
   cin>> n3;
   
   if(n1 >= n2 && n1 >= n3)
       cout << "Largest Value" << n1;
       else if(n2 >= n1 && n2 >= n3)
       cout << "Largest Value" << n2;
       else if(n3 >= n1 && n3 >= n2)
       cout << "Largest Value" << n3;
   
   return 0;
}
